    "# Define tokenize function\n",
    "def tokenize_Text(text):\n",
    "    tokens = nltk.word_tokenize(text.lower())\n",
    "    # Insert only words\n",
    "    tokens = [word for word in tokens if word[0].isalpha()]\n",
    "    return tokens"
   ]
